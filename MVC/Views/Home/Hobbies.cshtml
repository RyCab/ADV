<h1>Hobbies</h1>
<p>Top 5 Hobbies</p>
@{
    var hobbies = (string[])ViewData["Hobbies"];
    <ul>
        @foreach(var hob in hobbies)
        {
            <li>@hob</li>
        }
    </ul>
}

@{
    ViewData["Title"] = "Cities";
}
<h1>@ViewData["Title"]</h1>

<p>Top 5 Favorite Cities</p>

<ul>
    @{
        foreach(var city in ViewBag.Cities)
        {
            <li>@city</li>
        }
    }
</ul>
